{
  "name": "omni-planner",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "test": "(npx tsc || echo 'TypeScript failed') && (npx eslint . || echo 'ESLint failed') && cd backend/omniplanner && gradle && gradle clean pitest",
    "build-ios": "npx eas build -p ios --profile preview",
    "build-android": "npx eas build -p android --profile preview",
    "backend": "cd backend/omniplanner && gradle bootRun",
    "wdio": "wdio run ./wdio.conf.ts",
    "coverage": "nyc npm run test",
    "testUI": "mocha"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@react-native-async-storage/async-storage": "^1.23.1",

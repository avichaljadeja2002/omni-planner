    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "axios": "^1.7.7",
    "expo": "^52.0.46",
    "expo-auth-session": "^6.0.3",
    "expo-constants": "^17.0.3",
    "expo-linking": "^7.0.4",
    "expo-router": "^4.0.21",
    "expo-web-browser": "^14.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-native": "^0.76.9",
    "react-native-bouncy-checkbox": "^4.1.2",

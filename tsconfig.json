{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": true,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true, 
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "constants/constants.tsx"
, "app/log.js"  ]
}

{
  "expo": {
    "name": "omni-planner",
    "slug": "omni-planner",
    "scheme": "omni-planner",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.omniplanner",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.omniplanner",
      "versionCode": 1,
      "icon": "./assets/images/icon.png"
    },
    "config":{
      "apiKey": "AIzaSyB0hBnEFxRwAlfNraiJj7g9OCwE8VQ71uI"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#000000"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "5e38bdbb-79f1-4876-badc-e3b7a219d88b"
      }
    }
  }
}
